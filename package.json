{
  "name": "anamnesis",
  "version": "0.9.9",
  "description": "",
  "homepage": "https://github.com/  (hit Enter to use an empty string)/p3#readme",
  "bugs": {
    "url": "https://github.com/  (hit Enter to use an empty string)/p3/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/  (hit Enter to use an empty string)/p3.git"
  },
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "check": "biome check --error-on-warnings --no-errors-on-unmatched --staged . && npm run check-types --workspaces --if-present",
    "check:all": "biome check --error-on-warnings . && npm run check-types --workspaces --if-present",
    "check:staged": "biome check --error-on-warnings --no-errors-on-unmatched --staged .",
    "check:fix": "biome check --write --error-on-warnings --no-errors-on-unmatched --staged .",
    "fix:all": "biome check --write --error-on-warnings .",
    "clean": "node ./bin/clean",
    "db:migrate": "npm run db:migrate --workspace=server",
    "dev": "concurrently -c green,yellow -t \"HH:mm:ss\" -p \"{name} {time}\" \"npm:dev:*\"",
    "dev:client": "npm run dev --workspace=client",
    "dev:server": "npm run dev --workspace=server",
    "start": "npm run start --workspace=server",
    "test": "npm run test --workspaces --if-present",
    "prepare": "husky"
  },
  "keywords": [
    "monorepo",
    "react",
    "nodejs",
    "postgresql",
    "sequelize"
  ],
  "author": "  (hit Enter to use an empty string)",
  "license": "Apache-2.0",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "concurrently": "^9.1.2",
    "husky": "^9.1.7",
    "validate-branch-name": "^1.3.2"
  },
  "dependencies": {
    "nodemailer": "^7.0.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,css}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  }
}
